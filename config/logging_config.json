{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers": {
        "fileHandler": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "filename": "logs/test_log",
            "mode": "a",
            "encoding": "utf-8"
        }
    },
    "root": {
        "handlers": ["fileHandler"],
        "level": "INFO"
    }
}

